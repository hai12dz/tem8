                </div>


                
                @RenderBody()
               


                <div class="right_menu_corner"></div>
            </div>
            <!-- end of menu tab -->
     
            <!-- end of left content -->
        


            <div id="display" class="center_content">


            </div>

            <!-- end of center content -->
           
            <!-- end of right content -->
        </div>
        <!-- end of main content -->
      
        <partial name="Footer"/>


    </div>
    <!-- end of main_container -->
</body>
</html>
